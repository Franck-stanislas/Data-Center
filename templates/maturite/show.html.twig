{% extends 'base.html.twig' %}

{% block title 'Maturite' %}

{% block body %}
    <h1{%trans%}>Maturite{%endtrans%}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td> {{ maturite.id }} </td>
            </tr>
            <tr>
                <th>{%trans%}Nom de la maturité{%endtrans%}</th>
                <td>{{ maturite.nomMaturite }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_maturite_index') }}">{%trans%}Retour à la liste{%endtrans%}</a>

    <a href="{{ path('app_maturite_edit', {'id': maturite.id}) }}">{%trans%}Modifier{%endtrans%}</a>

    {{ include('maturite/_delete_form.html.twig') }}
{% endblock %}
