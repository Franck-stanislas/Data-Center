<div class="col-12 col-xl-8">
    <div class="card card-body border-0 shadow mb-4">
        {{ form_start(form) }}
        <div class="row">
            <div class="col-md-6 mb-3">
                <div>
                    <label for="first_name">{%trans%}Nom de l'élément{%endtrans%}</label>
                    {{ form_widget(form.nom|trans) }}
                </div>
            </div>

            <div class="col-md-6 mb-3">
                <div>
                    <label for="first_name">{%trans%}Maturité de l'élément{%endtrans%}</label>
                    {{ form_widget(form.idMaturite) }}
                </div>
            </div>
        </div>
        <div class="mt-3">
            <button class="btn btn-gray-800 mt-2 me-5 animate-up-2" type="submit">
                <i class='bx bx-save me-1'></i>
                {{ button_label|default('Enregistrer') }}
            </button>
            <a href="{{ path('app_elt_maturite_index') }} " class="btn btn-gray-800 mt-2 animate-up-2">
                <i class='bx bx-arrow-back me-1'></i> {%trans%}Retour à la liste{%endtrans%}
            </a>

        </div>
        {{ form_end(form) }}
    </div>
</div>

